{
  "name": "@examples/next-starter",
  "version": "9.27.2",
  "private": true,
  "scripts": {
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "yarn ts-node ./src/server.ts",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "run-s db-migrate-dev db-seed dev",
    "start:production": "yarn prisma migrate deploy && NODE_ENV=production node ./dist/server.js",
    "start:test": "next start",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "test-dev": "start-server-and-test dev 3000 test",
    "test-start": "start-server-and-test start 3000 test",
    "test": "run-s test:*",
    "test:unit": "jest",
    "ts-node": "ts-node -T",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
