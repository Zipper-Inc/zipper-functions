{
  "name": "@zipper/framework",
  "description": "The Zipper framework for Deno Deploy",
  "private": true,
  "version": "0.1.3",
  "scripts": {
    "build:deno": "deno run -A ./bin/generate.ts",
    "build:watch": "deno run -A ./bin/generate-watch.ts",
    "dev": "yarn build:watch & yarn --cwd=\"../..\" deno:dev ./packages/@zipper-framework/deno/run.ts port=8888",
    "lint": "biome lint --max-diagnostics=5000 --log-kind=compact --diagnostic-level=error ./"
  }
}
