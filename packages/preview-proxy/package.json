{
  "name": "preview-proxy",
  "version": "0.0.0",
  "description": "dev only proxy server",
  "main": "./main.ts",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "dev": "yarn nodemon",
    "start": "yarn ts-node ./main.ts"
  },
  "nodemonConfig": {
    "watch": [
      "main.ts"
    ],
    "exec": "yarn start",
    "ext": "ts, tsx"
  }
}
