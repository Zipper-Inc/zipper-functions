{
  "name": "zipper.run",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn dotenv:development -- -- next dev --port 3002",
    "dotenv:development": "dotenv -c $(sh ../../bin/dotenv.development.sh)",
    "dotenv:preview": "dotenv $(sh ../../bin/dotenv.preview.sh)",
    "dotenv:production": "dotenv $(sh ../../bin/dotenv.production.sh)",
    "build": "next build",
    "build:preview": "yarn dotenv:preview -- -- next build",
    "build:production": "yarn dotenv:production -- -- next build --no-lint",
    "start": "yarn playwright install; next start",
    "start:preview": "yarn playwright install; yarn dotenv:preview -- -- next start",
    "start:production": "yarn playwright install; yarn dotenv:production -- -- next start",
    "lint": "next lint"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "engines": {
    "node": "^18.14.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.391.0",
    "@aws-sdk/s3-request-presigner": "^3.391.0",
    "@aws-sdk/types": "^3.391.0",
    "@chakra-ui/react": "^2.4.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.15",
    "@sentry/nextjs": "^7.51.0",
    "@vercel/kv": "^1.0.0",
    "@zipper/types": "*",
    "@zipper/ui": "*",
    "@zipper/utils": "*",
    "chrono-node": "^2.5.0",
    "cookies-next": "^2.1.1",
    "framer-motion": "^9.0.0",
    "jose": "^4.11.4",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.4.17",
    "playwright": "^1.32.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@zipper/framework": "*",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
