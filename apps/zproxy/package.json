{
  "name": "zproxy",
  "version": "0.0.0",
  "description": "Zipper's internal dev proxy service",
  "main": "./main.ts",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "__dev": "yarn nodemon",
    "__start": "yarn ts-node ./main.ts"
  },
  "nodemonConfig": {
    "watch": [
      "main.ts"
    ],
    "exec": "yarn __start",
    "ext": "ts, tsx"
  },
  "dependencies": {
    "minimist-lite": "^2.2.1"
  }
}
